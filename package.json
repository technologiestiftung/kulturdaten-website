{
  "name": "kulturdaten-website",
  "version": "0.0.0",
  "description": "Our kulturdaten showcase website",
  "scripts": {
    "postinstall": "next telemetry disable",
    "generate-api": "openapi --input src/api/kulturdaten.berlin.openapi.generated.yml --output src/api/client --name APIClient",
    "dev": "next dev",
    "prebuild": "npm run generate-api",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write .",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/technologiestiftung/kulturdaten-website.git"
  },
  "keywords": [],
  "author": "Technologiestiftung Berlin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/technologiestiftung/kulturdaten-website/issues"
  },
  "homepage": "https://github.com/technologiestiftung/kulturdaten-website#readme",
  "dependencies": {
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "next": "13.4.7",
    "next-intl": "^2.17.5",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "7.0.25",
    "@storybook/addon-interactions": "7.0.25",
    "@storybook/addon-links": "7.0.25",
    "@storybook/nextjs": "7.0.25",
    "@types/jest": "29.5.2",
    "@types/node": "20.3.3",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "@typescript-eslint/eslint-plugin": "5.60.1",
    "@typescript-eslint/parser": "5.60.1",
    "eslint": "8.44.0",
    "eslint-config-next": "13.4.7",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-storybook": "0.6.12",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "openapi-typescript-codegen": "^0.25.0",
    "prettier": "2.8.8",
    "storybook": "7.0.25",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.1.6"
  }
